{
  "expo": {
    "name": "Phantom Notify",
    "slug": "phantom_notify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/phantom_reminder.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/phantom_reminder.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/phantom_reminder.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/phantom_reminder.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
